{
  "printWidth": 120,
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "printWidth": 200
      }
    }
  ],
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "proseWrap": "never",
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "ignore",
  "jsxSingleQuote": false,
  "quoteProps": "as-needed"
}
